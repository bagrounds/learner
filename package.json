{
  "name": "learner",
  "version": "0.2.3",
  "main": "learner.js",
  "private": true,
  "scripts": {
    "start": "node bin/learner",
    "test": "_mocha",
    "predocument": "rm -rf docs",
    "document": "jsdoc --destination docs -c .jsdoc.json -r",
    "precoverage": "rm -rf coverage",
    "coverage": "istanbul cover _mocha -- -R spec",
    "postcoverage": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "build": "npm run coverage && npm run document",
    "prepush": "npm run coverage",
    "push": "git push origin master",
    "lint": "eslint *.js lib/**/*.js test/**/*.js"
  },
  "dependencies": {
    "argparse": "^1.0.7",
    "configstore": "^2.0.0",
    "convnetjs": "^0.3.0",
    "lodash": "^4.11.2",
    "mathjs": "^3.2.1",
    "type-check": "^0.3.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.9.0",
    "istanbul": "^0.4.3",
    "jsdoc": "^3.4.0",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.2.0"
  }
}
